{
    "description": "Demonstrating webNavigation",
    "manifest_version": 2,
    "name": "webNavigation-demo",
    "version": "1.0",
    "permissions": ["webNavigation"],
    "background": {"scripts": ["background.js"]},
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}
